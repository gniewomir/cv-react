{
  "name": "cv-react",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules --include-path ./src/ src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./node_modules --include-path ./src/ src/ -o src/ --watch --recursive",
    "start-json": "node ./node_modules/json-server/bin/index.js --watch --port 3004 --routes ./public/data/routes.json ./public/data/db.json",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js start-json",
    "build": "yarn run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "animated-scroll-to": "^1.1.1",
    "axios": "^0.16.2",
    "bem-cn": "^2.1.3",
    "lodash.throttle": "^4.1.1",
    "normalize-scss": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "typographic": "^3.0.0"
  },
  "devDependencies": {
    "json-server": "^0.11.2",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2"
  }
}
