{
  "name": "cv-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules --include-path ./src/style/scss src/style/scss -o src/style/css",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./node_modules --include-path ./src/style/scss src/style/scss -o src/style/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bem-cn": "^2.1.3",
    "normalize-scss": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2",
    "typographic": "^3.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2"
  }
}
